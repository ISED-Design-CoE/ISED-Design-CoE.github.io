var IsedAdmin = function () {
  var initialized = false;   // Flag to indicate that this class has been initialized
  var form_required_valid = true; // Flag to indicate that the validation is for the News and EO forms
  var lang = 'en';           // Will be 'en' or 'fr' regardless of how the url segment is formed (currently eng or fra)
  var mouse = {x:0, y:0};    // Tracks the mouse position
  var page_type = 'content';
  var temp_val = 0;

  /**
   * Initialization
   */
  function init() {
    if (initialized) {
      return;
    }

    // Get the current UI language
    $ = jQuery;
    IsedAdmin.lang = $('html').attr('lang');

    //Determine the page type
    if ($('body').hasClass('path-node')) {
      IsedAdmin.page_type = 'content'; // Content pages generated by wxt sub theme (epic_theme?).
    }

    if ($('body').hasClass('path-frontpage')) {
      IsedAdmin.page_type = 'front'; // Front page <front> ex, /en or /fr.
    }
    else if ($('body').hasClass('node-edit')) {
      IsedAdmin.page_type = 'edit-node';
    }

    if ($('body').hasClass('node-edit') && $('body').hasClass('page-node-type-page')) {
      IsedAdmin.page_type = 'edit-page';
      syncModeNav();
    }
    else if ($('body').hasClass('node-edit') && $('body').hasClass('page-node-type-landing-page')) {
      IsedAdmin.page_type = 'edit-landing-page';
      syncModeNav();
    }
    else if ($('body').hasClass('node-add') && $('body').hasClass('node-add-landing_page')) {
      IsedAdmin.page_type = 'add-landing-page';
      syncModeNav();
    }
    else if ($('body').hasClass('node-add') && $('body').hasClass('node-add-page')) {
      IsedAdmin.page_type = 'add-page';
      syncModeNav();
    }
    else if ($('body').hasClass('path-webform')) {
      IsedAdmin.page_type = 'webforms';
    }

    $(document).on('mousemove', onMouseMove);

    if (IsedAdmin.page_type == 'add-page' || IsedAdmin.page_type == 'edit-page') {
      setLayoutDefaults();
      syncBodyEditorModes();
    }
    if (IsedAdmin.page_type == 'add-landing-page' || IsedAdmin.page_type == 'edit-landing-page') {
      setLayoutDefaults();
      syncBodyEditorModes();
    }

    if (IsedAdmin.isIE()) {
      $('body').addClass('isIE');
    }

    $('.alert a.access-unpublished').each(function (index, element) {
      if ($(this).hasClass('access-unpublished')) {
        // Add a copy-link icon to the unpublished link, if one exists
        var access_link = $('a.access-unpublished');
        if (access_link.length > 0) {
          var spn = $('<span class="glyphicon glyphicon-copy" style="margin-left: 10px" title="' + (IsedAdmin.lang == 'fr' ? 'Copier le lien' : 'Copy link') + '"></span>');
          $(access_link).parent().append(spn);
          $(spn).click(function () {
            // In prod the href is absolute, but in local environment href starts with a '/'.
            // This code ensures copy link is correct in all environments.
            var link = $(access_link).attr('href');
            if (!link.startsWith('http')) {
              link = window.location.origin + link;
            }
            IsedAdmin.copyLink(link);
          });
        }
      }
    });

    initialized = true;
  }

  /**
   * Keep track of mouse movements.
   */
  function onMouseMove(event) {
    IsedAdmin.mouse.x = event.clientX;
    IsedAdmin.mouse.y = event.clientY;
  }

  function syncBodyEditorModes() {
    var bothBodyFormats = $("#edit-body-0-format--2, #edit-body-etuf-fr-0-format--2");
    if (bothBodyFormats) {
      // If changing from rich text to full text, changes both.
      bothBodyFormats.on('blur', function (e) {
        IsedAdmin.temp_val = this.id;
        setTimeout(function () {
          var temp = IsedAdmin.temp_val;
          var tempVal = $('#' + IsedAdmin.temp_val).val();
          if (temp == 'edit-body-0-format--2') {
            temp = $('.form-item--body-0-format option:selected').val();
            var frenchValue = $('.form-item--body-etuf-fr-0-format option:selected').val();
            if (frenchValue != tempVal && typeof frenchValue !== 'undefined') {
              $('.form-item--body-etuf-fr-0-format select').val(tempVal);
              $('#edit-body-etuf-fr-0-format--2').trigger('change');// Activate language value
              console.info('syncBodyEditorModes() english');
            }
          }
          if (temp == 'edit-body-etuf-fr-0-format--2') {
            var englishValue = $('.form-item--body-0-format option:selected').val();
            temp = $('.form-item--body-etuf-fr-0-format option:selected').val();
            if (englishValue != tempVal && typeof englishValue !== 'undefined') {
              $('.form-item--body-0-format select').val(tempVal);// Set other language value.
              $('#edit-body-0-format--2').trigger('change');// Activate other language value.
              console.info('syncBodyEditorModes() français');
            }
          }
        }
        , 500);
      });
    }
  }

  function syncModeNav() {

    var showNav = $("#edit-field-show-nav-menu-value");
    if (showNav) {
      // If changing from rich text to full text, changes both.
      showNav.on('change', function (e) {
        //IsedAdmin.temp_val = this.id;
        var enable2Column = this.checked;
        if (enable2Column){
          $('#edit-menu').attr('open' , 'open');
          $('#edit-menu--2').css('display', 'block');
        }
        if(enable2Column && !($('#edit-menu-enabled').is(":checked"))) {
          setTimeout(function () {
            $("#edit-menu-enabled").click();
            $("#edit-menu-enabled").prop( "checked", true );
          }, 500);
        }
      });
    }
  }

  function setLayoutDefaults() {
        console.info('setLayoutDefaults()');
    //Layout - sync the layout type selection, with JS
    var bothSelects = $("#edit-layout-selection, #edit-layout-selection-etuf-fr, #edit-layout-selection-etuf-en");
    if (bothSelects) {
      bothSelects.change(function (e) {
        bothSelects.val(this.value); // "this" is the changed one
      });
    }

    // Set the layout defaults.
    var layoutElement = $("#edit-layout-selection");
    if (IsedAdmin.page_type == 'add-page') {
      layoutElement.val('node_page_default_default'); // Default to the default layout value.
      layoutElement.trigger('change');
    }
    if (IsedAdmin.page_type == 'add-landing-page') {
      layoutElement.val('node_landing_page_full_default'); // Default to the default layout value.
      layoutElement.trigger('change');
    }

    if (IsedAdmin.page_type == 'edit-landing-page') {
      if (layoutElement.val() == '_none') {
        layoutElement.val('node_landing_page_full_default'); // Default to the default layout value.
        layoutElement.trigger('change');
      }
    }
    if (IsedAdmin.page_type == 'edit-page') {
      if (layoutElement.val() == '_none') {
        layoutElement.val('node_page_default_default'); // Default to the default layout value.
        layoutElement.trigger('change');
      }
    }

  }

  function isIE() {
    if (/Edge/.test(navigator.userAgent)) {
      return false;
    }
    if (navigator.appName == 'Microsoft Internet Explorer') {
      return true;
    }
    else if (navigator.appName == 'Netscape') {
      if (navigator.appVersion.indexOf('Trident') > 0 || navigator.appVersion.indexOf('Edge') > 0) {
        return true;
      }
    }
    return false;
  }

  /**
   * Copy a link to the clipboard
   */
  function copyLink(url) {
    if (isIE()) {
      window.clipboardData.setData('Text', url);
    }
    else {
      var range = document.createRange();
      var tmpElem = $('<div>');
      tmpElem.text(url);
      $('body').append(tmpElem);
      range.selectNodeContents(tmpElem.get(0));
      selection = window.getSelection();
      selection.removeAllRanges();
      selection.addRange(range);
      document.execCommand('copy', false, null);
      tmpElem.remove();
    }

    // Display a popup message to indicate the link was copied, then fade it out
    lang = IsedAdmin.lang;
    var copied_msg = $('<div>' + (lang == 'fr' ? 'Copié' : 'Copied') + ' </div>').css({
      'position': 'fixed',
      'background-color': 'black',
      'border-radius': '6px',
      'color': 'white',
      'padding': '10px 15px',
      'top': IsedAdmin.mouse.y,
      'left': IsedAdmin.mouse.x,
      'z-index': 10700
    })
    $('body').append(copied_msg);
    setTimeout(function () {
      $(copied_msg).animate({'opacity': 0}, 1000, function () {
        $(this).remove();
      });
    }, 1000);
  }

  /**
   * Expose functions and variables
   */
  return {
    init: init,
    lang: lang,
    page_type: page_type,
    temp_val: temp_val,
    isIE: isIE,
    mouse: mouse,
    copyLink: copyLink,
  }
}();
;
